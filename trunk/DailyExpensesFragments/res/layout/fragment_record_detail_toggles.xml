<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <CheckBox
        android:id="@+id/detail_bookingType"
		android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enable"
        android:textOff="@string/record_detail_incomeOff"
        android:textOn="@string/record_detail_incomeOn" />

    <CheckBox
        android:id="@+id/detail_payState"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="true"
        android:text="payed"
        android:textOff="@string/record_detail_payStateOff"
        android:textOn="@string/record_detail_payStateOn" />

</merge>